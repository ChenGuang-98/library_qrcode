<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.library.dao.bean">
	<class name="Admin" table="library_admin" lazy="false">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>

		<property name="ano" length="20"></property>
		<property name="aname" length="50"></property>
		<property name="password" length="32"></property>
		<property name="cdate" type="timestamp" ></property>
		<property name="udate" type="timestamp"></property>
		<property name="yn" ></property>
		
	</class>
</hibernate-mapping>